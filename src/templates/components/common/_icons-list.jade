mixin icons-list
	- const icons = ['allosaurus', 'calendar', 'cerato', 'dino-marker', 'discord', 'fossil', 'gallery-icon', 'gallimimus', 'github', 'guanlong', 'iguanodon', 'list', 'map-point', 'ouranosaurus', 'parasaurolophus', 'plateosaurus', 'reddit', 'search', 'settings-work-tool', 'styracosaurus', 't-rex', 'triceratops', 'user', 'velociraptor']

	.my-5
		hr.hr
		.h3 Icons list:
		.row(style="font-size: 40px; fill: #fff")
			each icon in icons
				.col-4.col-md-2.col-lg-1(data-tippy-content=icon data-tippy-placement="top-start" data-tippy-interactive="true")
					+svg(icon)
		hr.hr
